{
  "name": "ChunkkHT",
  "version": "0.0.3",
  "slug": "ChunkkHT",
  "description": "Chunkk home tools makes your home easyer",
  "url": "https://chunkk.nl",
  "startup": "application",
  "image": "chunkk/webapp",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "config:rw"
  ],
  "ingress": true,
  "hassio_api": false,
  "hassio_role": "default",
  "homeassistant_api": false,
  "host_network": false,
  "options": {
    "log_level": "information",
    "app_config_folder": "/config/ChunkkHT",
    "app_assembly": ""
  },
  "schema": {
    "log_level": "match(^(trace|debug|information|warning|error|critical)$)",
  },
  "ports": {
    "80/tcp": 8631
  },
  "ports_description": {
    "80/tcp": "Web interface"
  }
}
{
  "name": "ChunkkHT",
  "version": "0.0.1",
  "slug": "ChunkkHT",
  "description": "ChunkkHT",
  "url": "https://chunkk.nl",
  "startup": "application",
  "image": "chunkk/webapp",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "config:rw"
  ],
  "ingress": false,
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "log_level": "information",
    "app_config_folder": "/config/ChunkkHT",
    "app_assembly": ""
  },
  "schema": {
    "log_level": "match(^(trace|debug|information|warning|error|critical)$)",
    "app_config_folder": "str",
    "app_assembly": "str?",
    "mqtt_host": "str?",
    "mqtt_port": "int(0,65535)?",
    "mqtt_username": "str?",
    "mqtt_password": "str?",
    "mqtt_discovery_prefix": "str?",
    "mqtt_use_tls": "bool?",
    "mqtt_allow_untrusted_certificates": "bool?"
  },
  "discovery": [
    "mqtt"
  ],
  "services": [
    "mqtt:want"
  ],
  "ports": {
    "80/tcp": 8635
  }
}